generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model articles {
  id    String   @id @default(auto()) @map("_id") @db.ObjectId
  v     Int      @map("__v")
  date  DateTime @db.Date
  title String
}

model comment {
  id    String   @id @default(auto()) @map("_id") @db.ObjectId
  v     Int      @map("__v")
  date  DateTime @db.Date
  title String
}


